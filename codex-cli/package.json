    "release:latest": "npm run build && npm publish --tag latest --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openai/codex.git"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "typescript": "^5.9.2"
  }
}
