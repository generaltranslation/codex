{
  "name": "@generaltranslation/codex",
  "version": "0.0.8-alpha.1",
  "license": "Apache-2.0",
  "bin": {
    "codex": "dist/codex.js"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "files": [
    "bin",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/sdk.d.ts",
      "import": "./dist/sdk.js",
      "default": "./dist/sdk.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "release": "npm run build && npm publish",
